 {
    "widgets": [
        {
            "type": "metric",
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 6,
            "properties": {
                "view": "timeSeries",
                "stacked": false,
                "metrics": [
                    [ "AWS/SQS", "ApproximateNumberOfMessagesVisible", "QueueName", "${SQSTaskQueueName}", { "region": "${AWS::Region}" } ],
                    [ ".", "ApproximateNumberOfMessagesNotVisible", ".", ".", { "region": "${AWS::Region}" } ]
                ],
                "region": "${AWS::Region}",
                "title": "SQS Queues",
                "period": 300
            }
        },
        {
            "type": "metric",
            "x": 0,
            "y": 12,
            "width": 12,
            "height": 6,
            "properties": {
                "metrics": [
                    [ "Turbine", "ClusterLoad", "StackName", "${TurbineStackName}" ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "${AWS::Region}",
                "title": "Cluster Load ",
                "period": 300,
                "stat": "Average"
            }
        },
        {
            "type": "metric",
            "x": 12,
            "y": 0,
            "width": 12,
            "height": 6,
            "properties": {
                "metrics": [
                    [ "AWS/EC2", "CPUUtilization", "AutoScalingGroupName", "${EC2AutoScalingGroupName}" ],
                    [ "...", "${EC2AutoScalingGroupNameWebserver}" ],
                    [ "...", "${EC2AutoScalingGroupNameWorker}" ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "${AWS::Region}",
                "title": "Credit Utilization (CPU)",
                "period": 300,
                "stat": "Average"
            }
        },
        {
            "type": "metric",
            "x": 12,
            "y": 6,
            "width": 12,
            "height": 6,
            "properties": {
                "metrics": [
                    [ "AWS/EC2", "CPUCreditBalance", "AutoScalingGroupName", "${EC2AutoScalingGroupName}", { "region": "${AWS::Region}" } ],
                    [ "...", "${EC2AutoScalingGroupNameWebserver}", { "region": "${AWS::Region}" } ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "${AWS::Region}",
                "title": "Credit Balance (CPU)",
                "period": 300,
                "stat": "Average"
            }
        },
        {
            "type": "metric",
            "x": 0,
            "y": 6,
            "width": 12,
            "height": 6,
            "properties": {
                "metrics": [
                    [ "AWS/EC2", "CPUCreditUsage", "AutoScalingGroupName", "${EC2AutoScalingGroupName}", { "region": "${AWS::Region}" } ],
                    [ "...", "${EC2AutoScalingGroupNameWebserver}", { "region": "${AWS::Region}" } ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "${AWS::Region}",
                "title": "Credit Usage (CPU)",
                "period": 300,
                "stat": "Average"
            }
        },
        {
            "type": "metric",
            "x": 12,
            "y": 12,
            "width": 12,
            "height": 6,
            "properties": {
                "view": "timeSeries",
                "stacked": false,
                "metrics": [
                    [ "AWS/EC2", "CPUUtilization", "AutoScalingGroupName", "${EC2AutoScalingGroupNameWorker}" ],
                    [ "...", "${EC2AutoScalingGroupNameWebserver}" ],
                    [ "...", "${EC2AutoScalingGroupName}" ]
                ],
                "region": "${AWS::Region}",
                "title": "CPU Utilization "
            }
        }
    ]
}